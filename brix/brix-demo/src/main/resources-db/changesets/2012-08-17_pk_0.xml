    <changeSet id="0" author="pavel.knorr">
        <comment>Migrate MIME types into Database</comment>

        <createTable schemaName="public" tableName="mime">
            <column name="mime_type" type="varchar(255)">
                <constraints primaryKey="true"/>
            </column>
            <column name="icon_name" type="varchar(255)">
                <constraints nullable="true"/>
            </column>
            <column name="resource_key" type="varchar(255)">
                <constraints nullable="true"/>
            </column>
        </createTable>
    </changeSet>

